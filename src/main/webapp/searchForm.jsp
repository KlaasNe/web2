<form method="post" action="Controller?command=search" class="search">
    <label for="search-bar"></label>
    <input type="text" placeholder="search item" id="search-bar" name="search-bar">
    <label for="search-select"></label>
    <select id="search-select" name="search-select">
        <option value="want">I want this item</option>
        <option value="have">I have this item</option>
    </select>
    <label for="search-btn"></label>
    <input type="submit" value="Search" id="search-btn">
</form>
